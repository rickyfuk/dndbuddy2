<nav class="navbar fixed-top navbar-light">
	<a class="navbar-brand" id="utl-user-guide" href="#">User Guide</a>
	<a class="navbar-brand" id="savetoDB" href="#">Save</a>
	<a class="navbar-brand" id="logout" href="/logout">Logout</a>
